{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "sourceMap": true,
        "jsx": "react"
    },
    "filesGlob": [
        "main.ts",
        "src/**/*.ts",
        "src/**/*.tsx"
    ],
    "files": [
        "main.ts",
        "src/App.tsx",
        "src/Canvas.tsx",
        "src/Footer.tsx",
        "src/init.tsx",
        "src/MenuBar.tsx"
    ],
    "atom": {
        "rewriteTsconfig": true
    }
}
